			branch
				include : WithAIHSerifsMask CAP 0 SB RightSB : union
					xStrand false SB 0 RightSB CAP 0.1 0.4 0.28
					xStrand false SB CAP RightSB 0 0.1 0.4 0.28
				include : AIHSerifs CAP
				save 'X.curly' 'X'
				save 'Chi.curly' 0x3A7
