		define [RevSmallEShape top stroke barpos] : glyph-construction
			local barbottom (top * [fallback barpos EBarPos] - HalfStroke)
			local hookx SB
			local hookmiddle : [mix (RightSB - O) hookx 0.55] + CorrectionOMidS

			include : dispiro
				widths.rhs stroke
